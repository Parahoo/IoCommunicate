<UserControl x:Class="WpfNetAssit.Communicate.Send.SendPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfNetAssit.Communicate.Send"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl TabStripPlacement="Left">
            <TabItem Header="常规">
                <local:NormalSendPage x:Name="NormalSendPage" VerticalAlignment="Top" DataSend="SendPage_DataSend"/>
            </TabItem>
            <TabItem Header="逻辑">
                <Grid>
                    <Border BorderThickness="1" BorderBrush="AliceBlue"></Border>
                    <local:LogicalSendPage x:Name="LogicalSendPage" Margin="1"/>   
                </Grid>             
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
